<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="demkobibl.GiveBookWindow"
        Title="GiveBookWindow">
    <StackPanel Orientation="Vertical" Spacing="15" VerticalAlignment="Center" HorizontalAlignment="Center">
        <ComboBox Width="350" Height="65" x:Name="ClientComboBox"/>
        <ComboBox Width="350" Height="65" x:Name="BookCopyComboBox"/>
        <DatePicker Width="350" Height="65" x:Name="IssueDatePicker"/>
        <DatePicker Width="350" Height="65" x:Name="EndDatePicker"/>
        <Button Width="350" Height="30" x:Name="GiveBookClientButton" Content="Сохранить" Click="GiveBookClientButton_OnClick" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Center"/>
    </StackPanel>
</Window>
